def area(radius):
	pi = 3.14
	return pi * radius * radius
radius = float(input("Enter the radius : "))
if radius < 0.0 or radius > 100.0:
	print("Enter a positive value upto 100")
elif radius == 0.0:
	print("Area of circle = 0.000000")
else:
	areav = area(radius)
	print(f"Area of circle = {areav:.6f}")
